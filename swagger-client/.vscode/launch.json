{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "React Management Run",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start"],
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "server_type": "local",
        },
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome For Debug",
      "url": "http://localhost:3000/react/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "=================================="
    }
  ]
}
